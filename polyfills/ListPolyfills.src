ListPolyfills.includes = function(item)
    for i in range(0, (self.len -1))
		if item == self[i] then
            return true
        end if
	end for
end function

for polyfill in ListPolyfills.indexes
    list[polyfill] = @ListPolyfills[polyfill]
end for